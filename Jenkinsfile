pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compiling Hello World'
            }
        }
        stage('UnitTest') {
            steps {
                echo 'Testing Hello World'
            }
        }
         stage('Package') {
            steps {
                echo 'Packaging Hello World'
            }
        }
    }
}
