            steps {
                echo 'Compiling the Code'
            }
        }
         stage('UnitTest') {
            steps {
                echo 'UnitTesting the Code'
            }
        }
         stage('Package') {
            steps {
                echo 'Packaging the Code'
            }
        }
    }
}
