pipeline {
    agent any
    
    stages {
        stage('compile') {
            steps {
                script{
                echo ' COMPILE-Hello World'
                }
            }
            
        }
         stage('UnitTest') {
            steps {
                script{
                echo 'UNIT-TESTHello World'
                }
            }
            
        }
         stage('package') {
            steps {
                script{
                echo 'PACKAGE-Hello World'
                }
            }
            
        }
    }
}
